.swiper {
  width: 100%;
  height: 200px;
}
.swiper image {
  width: 100%;
  height: 100%;
}
page {
  background: rgba(182, 174, 193, 0.74);
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;

}
.tupian {
  width: 100rpx;
  height: 100rpx;
  margin-top: 200rpx;
}
.title {
  font-size: 14px;
  font-family: 微软雅黑,Arial;
  margin-top: 100rpx;
}

.btn {
  width: 200rpx;
  height: 30px;
  border: 1px solid #000;
  border-radius: 5px;
  margin-top: 100rpx;
  text-align: center;
}
.btn text {
  font-size: 14px;
  line-height: 30px;
}
